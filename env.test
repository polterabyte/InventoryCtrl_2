# Test Environment Variables for test.warehouse.cuby

# Database Configuration
POSTGRES_DB=inventorydb_test
POSTGRES_USER=postgres
POSTGRES_PASSWORD=test_password_here

# JWT Configuration
JWT_KEY=test_jwt_key_at_least_32_characters_long_for_testing
JWT_ISSUER=InventoryServer
JWT_AUDIENCE=InventoryClient
JWT_EXPIRE_MINUTES=60
JWT_REFRESH_EXPIRE_DAYS=1

# Domain Configuration
DOMAIN=test.warehouse.cuby
STAGING_DOMAIN=staging.warehouse.cuby
TEST_DOMAIN=test.warehouse.cuby

# ASP.NET Core Configuration
ASPNETCORE_ENVIRONMENT=Test
ASPNETCORE_URLS=http://+:80

# SSL Configuration
SSL_CERT_PATH=/etc/nginx/ssl/test.warehouse.cuby.crt
SSL_KEY_PATH=/etc/nginx/ssl/test.warehouse.cuby.key

# Logging
LOG_LEVEL=Trace
LOG_FILE_PATH=/app/logs/test-{Date}.log
